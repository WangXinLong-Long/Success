<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#EEEEF3"
    tools:context="com.silianchuangye.sumao.success.fragments.firmInfomation.FirmInfoTypeActivity">
<include
    android:id="@+id/layoutTop_firm_info_type"
    layout="@layout/title_bar"></include>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x130"
        android:background="#fff"
        android:layout_marginTop="@dimen/x5"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <RadioButton
                android:layout_width="@dimen/x30"
                android:layout_height="@dimen/x20"
                android:id="@+id/rbone_firm_info_type"
                android:layout_marginTop="@dimen/x10"
                android:layout_marginLeft="@dimen/x10"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x10"
                android:layout_marginLeft="@dimen/x10"
                android:text="生产商"
                android:textSize="16sp"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#EEEEF3"
            android:layout_marginTop="@dimen/x10"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            ></LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RadioButton
                android:layout_width="@dimen/x30"
                android:layout_height="@dimen/x20"
                android:id="@+id/rbtwo_firm_info_type"
                android:layout_marginTop="@dimen/x10"
                android:layout_marginLeft="@dimen/x10"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x10"
                android:layout_marginLeft="@dimen/x10"
                android:text="贸易商"
                android:textSize="16sp"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#EEEEF3"
            android:layout_marginTop="@dimen/x10"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            ></LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RadioButton
                android:layout_width="@dimen/x30"
                android:layout_height="@dimen/x20"
                android:id="@+id/rbthree_firm_info_type"
                android:layout_marginTop="@dimen/x10"
                android:layout_marginLeft="@dimen/x10"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x10"
                android:layout_marginLeft="@dimen/x10"
                android:text="贸易商"
                android:textSize="16sp"
                />
        </LinearLayout>
    </LinearLayout>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/x10"
        android:layout_marginLeft="@dimen/x10"
        android:layout_marginTop="@dimen/x10"
        android:background="@drawable/button_background_blue"
        android:textColor="#fff"
        android:textSize="16sp"
        android:text="保存"

        />

</LinearLayout>
