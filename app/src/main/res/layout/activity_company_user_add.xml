<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.silianchuangye.sumao.success.fragments.companyInfomation.CompanyUserAddActivity">

    <include
        android:id="@+id/layouttop_add"
        layout="@layout/title_bar"></include>
    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/lvAdd_company_user"
        android:layout_below="@+id/layouttop_add"
        ></ListView>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btsave_company_user_add"
        android:layout_below="@+id/lvAdd_company_user"
        android:background="@drawable/button_background_blue"
        android:layout_marginTop="@dimen/x10"
        android:layout_marginLeft="@dimen/x10"
        android:layout_marginRight="@dimen/x10"
        android:text="保存"
        android:textColor="#fff"
        android:textSize="15sp"
        />
</RelativeLayout>
