<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.silianchuangye.sumao.success.fragments.SellerManagementPlatform.acutionManagement.AcutionResultActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:background="#29ABE2"
        android:layout_height="@dimen/x43"
        android:id="@+id/layout_g"
        >

        <ImageView
            android:layout_width="@dimen/x20"
            android:layout_height="@dimen/x20"
            android:id="@+id/iv_Back_acution_Result"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x10"
            android:src="@mipmap/back"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textColor="#fff"
            android:textSize="16sp"
            android:text="竞拍结果查看"
            />
        <ImageView
            android:layout_width="@dimen/x20"
            android:layout_height="@dimen/x20"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x10"
            android:layout_centerVertical="true"
            android:src="@mipmap/search"
            android:id="@+id/iv_auction_result_search"
            />
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x5"
        android:background="#EEEEF3"
        android:layout_below="@+id/layout_g"
        android:id="@+id/Line_auction_result"
        android:orientation="horizontal"
        ></LinearLayout>
    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Line_auction_result"
        android:id="@+id/lv_acution_result"

        ></ListView>
</RelativeLayout>
