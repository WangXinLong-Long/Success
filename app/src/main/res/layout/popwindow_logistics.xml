<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <include
        layout="@layout/logistics_title_bar">

    </include>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <EditText
            android:id="@+id/edt_pop_logistics_product_order_num"
            android:layout_width="200dp"
            android:layout_height="35dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:layout_centerVertical="true"
            android:background="@drawable/juxing"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="产品订单编号 : "
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/edt_pop_logistics_product_order_num"
            android:textSize="16sp"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <EditText
            android:id="@+id/edt_pop_logistics_cangku"
            android:layout_width="200dp"
            android:layout_height="35dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="30dp"
            android:layout_centerVertical="true"
            android:background="@drawable/juxing"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="仓库 : "
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/edt_pop_logistics_cangku"
            android:textSize="16sp"
            android:layout_marginRight="10dp"/>
    </RelativeLayout>
    <RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="45dp">
    <EditText
        android:id="@+id/edt_pop_logistics_product_name"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="30dp"
        android:layout_centerVertical="true"
        android:background="@drawable/juxing"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="产品名称 : "
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@id/edt_pop_logistics_product_name"
        android:textSize="16sp"
        android:layout_marginRight="10dp"/>
</RelativeLayout>
    <RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="45dp">
    <EditText
        android:id="@+id/edt_pop_logistics_company"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="30dp"
        android:layout_centerVertical="true"
        android:background="@drawable/juxing"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="销售公司 : "
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@id/edt_pop_logistics_company"
        android:textSize="16sp"
        android:layout_marginRight="10dp"/>
</RelativeLayout>
    <RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="45dp">
    <EditText
        android:id="@+id/edt_pop_logistics_order_num"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="30dp"
        android:layout_centerVertical="true"
        android:background="@drawable/juxing"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="订单编号 : "
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@id/edt_pop_logistics_order_num"
        android:textSize="16sp"
        android:layout_marginRight="10dp"/>
</RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp">
    </RelativeLayout>



</LinearLayout>
