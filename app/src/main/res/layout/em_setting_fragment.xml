<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFEFF4" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#EFEFF4"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/btn_image_title"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_centerHorizontal="true"
            android:background="#ED5485"
            android:gravity="center"
            android:text="@string/setting"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />

        <RelativeLayout
            android:id="@+id/ll_setting_list_appkey"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_below="@id/btn_image_title"
            android:layout_marginTop="10dp"
            android:background="#FFFFFF"
            android:onClick="startAppKeyedit"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textview_appkey"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="15dp"
                android:gravity="center_vertical"
                android:text="AppKey  "
                android:textColor="#000000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/tv_setting_appkey"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="20dp"
                android:layout_toRightOf="@id/textview_appkey"
                android:layout_toLeftOf="@+id/iv_appkey_right"
                android:gravity="center_vertical"
                android:singleLine="true"
                android:ellipsize="end"
                android:text="sipsoft#sandbox"
                android:textColor="#9B9B9B"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/iv_appkey_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:background="@drawable/em_btn_back" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:layout_alignParentTop="true"
                android:background="#D6D5DB" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:layout_alignParentBottom="true"
                android:background="#D6D5DB" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ll_setting_list_account"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_marginTop="20dp"
            android:background="#FFFFFF"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textview_accountnumber"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="15dp"
                android:gravity="center_vertical"
                android:text="@string/customernumber"
                android:textColor="#000000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/tv_setting_account"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="20dp"
                android:layout_toRightOf="@id/textview_accountnumber"
                android:layout_toLeftOf="@+id/iv_account_right"
                android:gravity="center_vertical"
                android:singleLine="true"
                android:ellipsize="end"
                android:textColor="#9B9B9B"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/iv_account_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:background="@drawable/em_btn_back" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:layout_alignParentTop="true"
                android:background="#D6D5DB" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:layout_alignParentBottom="true"
                android:background="#D6D5DB" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ll_setting_list_nick"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_marginTop="20dp"
            android:background="#FFFFFF"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textview_nick"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="15dp"
                android:gravity="center_vertical"
                android:text="@string/set_nick"
                android:textColor="#000000"
                android:textSize="17sp" />

            <TextView
                android:id="@+id/tv_setting_nick"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_marginLeft="20dp"
                android:layout_toRightOf="@id/textview_nick"
                android:layout_toLeftOf="@+id/iv_nick_right"
                android:gravity="center_vertical"
                android:singleLine="true"
                android:ellipsize="end"
                android:textColor="#9B9B9B"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/iv_nick_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:background="@drawable/em_btn_back" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:layout_alignParentTop="true"
                android:background="#D6D5DB" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:layout_alignParentBottom="true"
                android:background="#D6D5DB" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/ll_setting_list_customer"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_marginTop="20dp"
            android:background="#FFFFFF"
            android:onClick="contactCustomer"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="15dp"
                android:gravity="center_vertical"
                android:text="@string/feedback"
                android:textColor="#000000"
                android:textSize="17sp" />

            <ImageView
                android:id="@+id/imageview_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="15dp"
                android:background="@drawable/em_btn_back" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:layout_alignParentTop="true"
                android:background="#D6D5DB" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dip"
                android:layout_alignParentBottom="true"
                android:background="#D6D5DB" />
        </RelativeLayout>
    </LinearLayout>

</FrameLayout>