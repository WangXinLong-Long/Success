<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="@dimen/x36"
    android:layout_width="match_parent"
    android:paddingTop="@dimen/padding_right"
    android:paddingBottom="@dimen/padding_right"
    android:background="@color/textColor_expandable_listview_show"
    >
    <ImageView
        android:id="@+id/iv_child_title_bar_back"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/x20"
        android:src="@mipmap/back"
        android:layout_marginRight="@dimen/x20"
        android:paddingLeft="@dimen/x10"
        android:paddingRight="@dimen/x15"
        android:layout_centerVertical="true"
        />
    <TextView
        android:id="@+id/tv_child_title_bar_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="@dimen/x20"
        android:layout_centerInParent="true"
        android:textColor="@color/white"
        />
</RelativeLayout>