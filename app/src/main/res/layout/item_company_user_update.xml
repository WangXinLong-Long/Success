<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#fff"
    android:layout_height="@dimen/x40">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvText"
        android:text="登录账号"
        android:layout_marginLeft="@dimen/x5"
        android:layout_marginTop="@dimen/x10"
        android:textSize="16sp"
        />
    <ImageView
        android:layout_width="@dimen/x20"
        android:layout_height="@dimen/x20"
        android:id="@+id/ivMore_company_user_update"
        android:layout_marginTop="@dimen/x10"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/x5"
        android:src="@mipmap/my_sumao_iv_order"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvValue_company_user_update"
        android:layout_marginTop="@dimen/x10"
        android:layout_toLeftOf="@+id/ivMore_company_user_update"
        android:layout_marginRight="@dimen/x5"
        android:textSize="16sp"

        android:layout_marginBottom="@dimen/x10"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvValue_company_user_update_passWord"
        android:layout_marginTop="@dimen/x10"
        android:layout_toLeftOf="@+id/ivMore_company_user_update"
        android:layout_marginRight="@dimen/x5"
        android:textSize="16sp"


        android:password="true"
        />

</RelativeLayout>
