<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <include
        android:id="@+id/view_logistics_demand_activity_title"
        layout="@layout/screen_title_bar"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="match_parent">
            <com.silianchuangye.sumao.success.custom.CustomExpandableListView
                android:layout_width="match_parent"
                android:id="@+id/listview"
                android:dividerHeight="@dimen/x5"
                android:headerDividersEnabled="false"
                android:layout_height="match_parent"></com.silianchuangye.sumao.success.custom.CustomExpandableListView>
        </LinearLayout>
    </ScrollView>

</LinearLayout>