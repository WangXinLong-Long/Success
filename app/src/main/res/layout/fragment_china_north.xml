<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/selley_bg">

    <!-- TODO: Update blank fragment layout -->
   <ListView
       android:layout_marginTop="@dimen/x10"
       android:id="@+id/lv_chainanorth"

       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:dividerHeight="@dimen/x5"
       android:divider="@color/selley_bg"
       android:background="@color/selley_lv">

   </ListView>

    <RelativeLayout
        android:id="@+id/relative_chinanorth"
        android:layout_marginTop="@dimen/x5"
        android:layout_below="@id/lv_chainanorth"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/selley_lv">
        <TextView
            android:id="@+id/tv_chinanorth"
            android:layout_width="match_parent"
            android:layout_height="@dimen/x40"
            android:background="@color/selley_lv"
            android:text="点价流程、点价规则"
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/x10"/>
        <ImageView
            android:id="@+id/img_chinanorth"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/x25"
            android:layout_alignParentRight="true"
            android:src="@mipmap/my_sumao_iv_order"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/x20"/>
    </RelativeLayout>

</RelativeLayout>
