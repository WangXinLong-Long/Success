    <Button
        android:layout_width="match_parent"
        android:layout_height="@dimen/x50"
        android:id="@+id/bt_lijibaojia"
        android:text="立即报价"
        android:textColor="#fff"
        android:background="@drawable/button_background_blue"
        android:textSize="17sp"
        />


</LinearLayout>
